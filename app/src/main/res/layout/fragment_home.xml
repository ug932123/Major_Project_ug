<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/home_back"
    tools:context=".HomeFragment">

    <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_gravity="center">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <Button
                    android:layout_width="160dp"
                    android:layout_height="160dp"
                    android:background="@drawable/home_btn"
                    android:layout_margin="5dp"
                    android:id="@+id/home_efficientnetb7"
                    android:text="EfficientNetB7"
                    android:textColor="@color/white"
                    android:fontFamily="@font/montserrat_regular"/>
                <Button
                    android:layout_width="160dp"
                    android:layout_height="160dp"
                    android:background="@drawable/home_btn"
                    android:layout_margin="5dp"
                    android:id="@+id/home_efficientnetb3"
                    android:text="EfficientNetB3"
                    android:textColor="@color/white"
                    android:fontFamily="@font/montserrat_regular"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <Button
                    android:layout_width="160dp"
                    android:layout_height="160dp"
                    android:background="@drawable/home_btn"
                    android:layout_margin="5dp"
                    android:id="@+id/home_vgg16"
                    android:text="VGG16"
                    android:textColor="@color/white"
                    android:fontFamily="@font/montserrat_regular"/>
                <Button
                    android:layout_width="160dp"
                    android:layout_height="160dp"
                    android:background="@drawable/home_btn"
                    android:layout_margin="5dp"
                    android:id="@+id/home_resnet50v2"
                    android:text="ResNet50v2"
                    android:textColor="@color/white"
                    android:fontFamily="@font/montserrat_regular"/>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <Button
                    android:layout_width="160dp"
                    android:layout_height="160dp"
                    android:background="@drawable/home_btn"
                    android:layout_margin="5dp"
                    android:id="@+id/home_about"
                    android:text="About Us"
                    android:textColor="@color/white"
                    android:fontFamily="@font/montserrat_regular"/>
                <Button
                    android:layout_width="160dp"
                    android:layout_height="160dp"
                    android:background="@drawable/home_btn"
                    android:layout_margin="5dp"
                    android:id="@+id/home_logout"
                    android:text="Logout"
                    android:textColor="@color/white"
                    android:fontFamily="@font/montserrat_regular"/>
            </LinearLayout>



        </LinearLayout>




</FrameLayout>